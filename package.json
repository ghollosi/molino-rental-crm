{
  "name": "molino-rental-crm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run session:start && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:log": "tsx src/scripts/dev-helper.ts",
    "docs:generate": "typedoc --out docs/api src",
    "docs:progress": "echo '## Session $(date +%Y-%m-%d)' >> PROGRESS.md",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "test": "vitest",
    "test:e2e": "playwright test",
    "type-check": "tsc --noEmit",
    "session:start": "tsx src/scripts/session-recovery.ts start",
    "session:save": "tsx src/scripts/session-recovery.ts save",
    "session:recover": "tsx src/scripts/session-recovery.ts recover",
    "checkpoint:create": "bash src/scripts/checkpoint.sh create",
    "checkpoint:restore": "bash src/scripts/checkpoint.sh restore",
    "status": "tsx src/scripts/project-status.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@hookform/resolvers": "^5.0.1",
    "@prisma/client": "^6.8.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.77.2",
    "@trpc/client": "^11.1.2",
    "@trpc/next": "^11.1.2",
    "@trpc/react-query": "^11.1.2",
    "@trpc/server": "^11.1.2",
    "@types/bcryptjs": "^2.4.6",
    "@uploadthing/react": "^7.3.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "exceljs": "^4.4.0",
    "i18next": "^25.2.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.511.0",
    "next": "15.3.2",
    "next-auth": "^4.24.11",
    "next-i18next": "^15.4.2",
    "next-pwa": "^5.6.0",
    "prisma": "^6.8.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.4",
    "react-i18next": "^15.5.2",
    "resend": "^4.5.1",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.0",
    "uploadthing": "^7.7.2",
    "zod": "^3.25.28",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.4",
    "typescript": "^5"
  }
}
