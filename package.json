{
  "name": "molino-rental-crm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3333",
    "dev:with-session": "npm run session:start && next dev -p 3333",
    "build": "prisma generate && next build",
    "vercel-build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "dev:log": "tsx src/scripts/dev-helper.ts",
    "docs:generate": "typedoc --out docs/api src",
    "docs:progress": "echo '## Session $(date +%Y-%m-%d)' >> PROGRESS.md",
