    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:components": "jest --testPathPattern=components",
    "test:utils": "jest --testPathPattern=utils",
    "test:api": "jest --testPathPattern=api",
    "test:ci": "jest --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "type-check": "tsc --noEmit",
    "session:start": "tsx src/scripts/session-recovery.ts start",
    "session:save": "tsx src/scripts/session-recovery.ts save",
    "session:recover": "tsx src/scripts/session-recovery.ts recover",
    "checkpoint:create": "bash src/scripts/checkpoint.sh create",
    "checkpoint:restore": "bash src/scripts/checkpoint.sh restore",
    "status": "tsx src/scripts/project-status.ts",
    "deploy:production": "./scripts/deploy-production.sh",
